	-webkit-animation: ratingBar 4s ease-in-out infinite;
	-moz-animation: ratingBar 4s ease-in-out infinite;
}
.inBar {
	top: 0px;
	left: 0px;
	padding: 0px;
	margin: 0px;
	color: red;
	font-size: 18px;
}
@-webkit-keyframes ratingBar {
	from {width:70%;}
	10%  {width:20%;}
	20%  {width:90%;}
	30%  {width:60%;}
	40%  {width:100%;}
	50%  {width:10%;}
	60%  {width:30%;}
	70%  {width:20%;}
	80%  {width:50%;}
	90%  {width:10%;}
	to   {width:70%;}										
}

